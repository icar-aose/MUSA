<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="cnr.icar.db.hibernate.Utente" table="Utenti">  
    <id name="id">  
     <generator class="native"></generator>  
    </id>  
            
    <property name="nome"  column="nome" type="string" ></property>  
    <property name="cognome"  column="cognome" type="string"></property>  
    <property name="email"  column="email" type="string"></property>  
    <property name="partitaIVA"  column="partitaIVA" type="long"></property>
    <property name="telefono"  column="telefono" type="long"></property>   
    <property name="username"  column="username" type="string"></property> 
    <property name="password"  column="password" type="string"></property>  
    <property name="infocloud"  column="infocloud" type="string"></property>  
    <property name="amministratore"  column="amministratore" type="byte"></property>  
    <property name="ruolo"  column="ruolo" type="int"></property>      
  </class>  
            
 </hibernate-mapping> 